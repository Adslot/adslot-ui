{
  "name": "adslot-ui",
  "version": "23.0.1",
  "description": "Core component library. By Adslot",
  "main": "dist/adslot-ui-main.js",
  "license": "MIT",
  "scripts": {
    "check-commit": "node scripts/commit-msg",
    "clean": "rimraf dist/*",
    "codecov": "cat coverage/coverage-final.json | codecov",
    "dist": "webpack --bail --env=dist --progress -p --hide-modules",
    "eslint": "eslint --ext .jsx,.js --rulesdir ./src ./src",
    "lint": "npm run eslint && npm run sass-lint && npm run check-commit",
    "posttest": "npm run lint",
    "postversion": "npm publish && git push --follow-tags && echo '…released.'",
    "preversion": "echo 'Releasing…' && git checkout master && git pull && npm i && npm prune",
    "profile": "webpack --env=dist --profile --json > stats.json",
    "release:major": "npm version major -m 'Release: major version %s.'",
    "release:minor": "npm version minor -m 'Release: minor version %s.'",
    "release:patch": "npm version patch -m 'Release: patch version %s.'",
    "sass-lint": "sass-lint './src/**/*.scss' -v",
    "start": "npm run start:dev",
    "start:dev": "webpack-dev-server --config=./config/dev.js",
    "start:cold": "node server.js --env=dev-cold",
    "test": "scripts/test --coverage",
    "test:watch": "scripts/test --watch",
    "version": "echo '…generating dist…' && npm run -s dist && git add dist/*",
    "scaffold": "./scripts/scaffold"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Adslot/adslot-ui.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "UI Library",
    "components"
  ],
  "author": "Adslot",
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-strict-mode": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "bootstrap-sass": "^3.3.7",
    "chai": "^4.1.1",
    "classnames": "^2.2.5",
    "codecov.io": "^0.1.6",
    "css-loader": "^0.28.4",
    "enzyme": "^2.9.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-lodash": "^2.4.4",
    "eslint-plugin-react": "=6.5.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "gonzales-pe": "^4.2.2",
    "istanbul-instrumenter-loader": "^3.0.0",
    "karma": "^1.7.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.4",
    "karma-webpack": "^2.0.4",
    "minimist": "^1.2.0",
    "mocha": "^3.5.2",
    "moment": "^2.18.1",
    "node-sass": "^4.5.3",
    "nuka-carousel": "^2.0.4",
    "null-loader": "^0.1.1",
    "object-assign": "^4.1.1",
    "open-browser-webpack-plugin": "0.0.5",
    "postcss-loader": "^2.0.6",
    "prop-types": "^15.5.10",
    "react-bootstrap": "^0.31.1",
    "react-datepicker": "^0.54.0",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-icheck": "^0.3.8",
    "react-redux": "^4.4.8",
    "react-select": "^1.0.0-rc.10",
    "react-test-renderer": "^15.6.1",
    "redux": "^3.7.2",
    "rimraf": "^2.6.1",
    "sass-lint": "^1.11.1",
    "sass-loader": "^6.0.6",
    "seamless-immutable": "^7.1.2",
    "semver": "^5.4.1",
    "shelljs": "^0.7.8",
    "sinon": "^4.0.0",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.6",
    "webpack-dev-server": "^2.8.1",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "diff-match-patch": "^1.0.0",
    "lodash": "^4.17.4",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "svg4everybody": "^2.1.9"
  },
  "engines": {
    "node": "6"
  }
}
