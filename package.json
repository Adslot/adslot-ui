    "babel-preset-react": "^6.16.0",
    "bootstrap-sass": "^3.3.7",
    "chai": "^3.5.0",
    "classnames": "^2.2.5",
    "codecov.io": "^0.1.6",
    "css-loader": "^0.26.1",
    "enzyme": "^2.6.0",
    "eslint": "^3.11.1",
    "eslint-config-airbnb": "^10.0.1",
