    "karma-coverage": "^1.0.0",
    "karma-mocha": "^1.0.1",
    "karma-mocha-reporter": "^2.0.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-phantomjs-shim": "^1.2.0",
    "karma-webpack": "^1.7.0",
    "minimist": "^1.2.0",
    "mocha": "^2.5.3",
    "moment": "^2.12.0",
    "node-sass": "^3.4.2",
    "null-loader": "^0.1.1",
    "object-assign": "^4.0.1",
