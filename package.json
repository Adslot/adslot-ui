{
  "name": "adslot-ui",
  "version": "32.1.0",
  "description": "Core component library. By Adslot",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "es/index.d.ts",
  "files": [
    "dist",
    "src/styles",
    "lib",
    "es"
  ],
  "license": "MIT",
  "scripts": {
    "generate-docs": "node ./scripts/generateDocs.js",
    "clean": "rimraf dist/* docs/* lib/* es/*",
    "generate-types": "node ./scripts/generate-types/generateTypes.js",
    "copy-types": "npm run generate-types -- -c",
    "type-check": "npx -p typescript tsc --allowSyntheticDefaultImports",
    "deps": "ADBLOCK=true DISABLE_OPENCOLLECTIVE=true npm ci",
    "dist:prod": "cross-env NODE_ENV=dist node ./scripts/build.js",
    "dist:lib": "cross-env NODE_ENV=cjs node ./scripts/babel-build.js",
    "dist:es": "cross-env NODE_ENV=esm node ./scripts/babel-build.js",
    "dist:demo": "cross-env NODE_ENV=production DEMO_ASSETS=true node ./scripts/build.js",
    "dist": "npm run clean && npm run copy-types && npm run dist:lib && npm run dist:es && npm run dist:prod && npm run generate-docs && npm run dist:demo",
    "lint:eslint": "eslint --ext .jsx,.js,.ts ./src && eslint --ext .jsx,.js ./www",
    "lint:stylelint": "stylelint '{src,www}/**/*.css'",
    "lint:prettier": "prettier '{src,www,config}/**/*.{js,json,jsx,md,mdx,css}' --check",
    "format": "prettier '{src,www,config}/**/*.{js,json,jsx,md,mdx,css}' --write",
    "lint": "npm run lint:eslint && npm run lint:prettier && npm run lint:stylelint",
    "prettier": "prettier \"{src,www,config}/**/*.{js,json,jsx,md,mdx,css}\" --write",
    "svgo": "svgo --recursive --multipass --folder=src --config=svgo-config.js",
    "postversion": "git push -u origin $(git rev-parse --abbrev-ref HEAD) --follow-tags && npm publish && echo '…released.'",
    "preversion": "echo 'Releasing…' && ADBLOCK=true DISABLE_OPENCOLLECTIVE=true npm ci",
    "release:major": "npm version major -m 'build: release major version %s'",
    "release:minor": "npm version minor -m 'build: release minor version %s'",
    "release:patch": "npm version patch -m 'build: release patch version %s'",
    "start": "npm run generate-docs && node scripts/start.js",
    "test": "jest --watch",
    "coverage": "jest --colors --coverage",
    "version": "echo '…generating dist…' && npm run dist",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Adslot/adslot-ui.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 120,
    "arrowParens": "always"
  },
  "lint-staged": {
    "{src,www}/**/*.css": [
      "stylelint --fix"
    ],
    "{src,www,config}/**/*.{js,json,jsx,md,mdx,css}": [
      "prettier --write"
    ],
    "**/*.ts": [
      "npm run type-check"
    ]
  },
  "keywords": [
    "UI Library",
    "components"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "author": "Adslot",
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.3",
    "@babel/generator": "^7.19.3",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.19.1",
    "@babel/preset-env": "^7.19.3",
    "@babel/preset-react": "^7.18.6",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@mdx-js/loader": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^13.5.0",
    "autoprefixer": "^10.4.12",
    "axios": "^1.1.0",
    "babel-jest": "^29.1.2",
    "babel-loader": "^8.2.5",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "babel-plugin-istanbul": "^6.1.1",
    "babel-plugin-jsx-remove-data-test-id": "^3.0.0",
    "babel-plugin-transform-remove-imports": "^1.7.0",
    "buffer": "^6.0.3",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "classnames": "^2.3.2",
    "commander": "^9.4.1",
    "core-js": "^3.25.5",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.2.1",
    "date-fns": "^2.29.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^8.24.0",
    "eslint-config-adslot": "^1.4.0",
    "eslint-plugin-jest-dom": "^4.0.2",
    "glob": "^8.0.3",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "jest": "^29.1.2",
    "jest-environment-jsdom": "^29.1.2",
    "lint-staged": "^13.0.3",
    "mini-css-extract-plugin": "^2.6.1",
    "null-loader": "^4.0.1",
    "postcss": "^8.4.17",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-import": "^14.1.0",
    "postcss-loader": "^7.0.1",
    "postcss-preset-env": "^7.8.2",
    "postcss-safe-parser": "^6.0.0",
    "postcss-simple-vars": "^6.0.3",
    "prettier": "^2.7.1",
    "prism-react-renderer": "^1.3.5",
    "prop-types": "^15.8.1",
    "react-dev-utils": "^12.0.1",
    "react-docgen": "^5.4.3",
    "react-docgen-displayname-handler": "^3.0.2",
    "react-html-parser": "^2.0.2",
    "react-live": "^2.4.1",
    "react-router-dom": "^6.3.0",
    "react-to-typescript-definitions": "^3.1.1",
    "remark-emoji": "^2.2.0",
    "rimraf": "^3.0.2",
    "seamless-immutable": "^7.1.4",
    "style-loader": "^3.3.1",
    "stylelint": "^14.13.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^28.0.0",
    "svg4everybody": "^2.1.9",
    "svgo": "^2.8.0",
    "terser-webpack-plugin": "^5.3.6",
    "url-search-params-polyfill": "^8.1.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@draft-js-plugins/editor": "^4.1.3",
    "@draft-js-plugins/mention": "^5.2.1",
    "@types/draft-js": "^0.11.9",
    "@types/react-datepicker": "^4.4.2",
    "@types/react-slick": "^0.23.10",
    "cropperjs": "^1.5.12",
    "diff-match-patch": "^1.0.5",
    "draft-js": "^0.11.7",
    "draft-js-export-html": "^1.4.1",
    "draft-js-import-html": "^1.4.1",
    "html-to-text": "^8.2.1",
    "react-datepicker": "^4.8.0",
    "react-popper": "^2.3.0",
    "react-select": "^5.4.0",
    "react-slick": "^0.29.0",
    "react-toastify": "^8.2.0",
    "slick-carousel": "^1.8.1"
  },
  "peerDependencies": {
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "react": "^16.14.0",
    "react-dom": "^16.14.0"
  },
  "engines": {
    "node": "^16"
  },
  "browserslist": [
    "last 2 versions",
    "> 5%",
    "not dead",
    "not kaios > 0",
    "not and_chr > 0",
    "not and_ff > 0",
    "not and_qq > 0",
    "not and_uc > 0",
    "not op_mob > 0",
    "not android > 0",
    "not ie_mob > 0",
    "not ios_saf > 0",
    "not op_mini > 0"
  ]
}
