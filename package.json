    "file-loader": "^0.11.2",
    "generator-react-webpack": "^3.3.4",
    "isparta-loader": "^2.0.0",
    "karma": "^1.7.0",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-phantomjs-shim": "^1.4.0",
    "karma-webpack": "^1.8.1",
    "minimist": "^1.2.0",
    "mocha": "^3.4.2",
    "moment": "^2.18.1",
    "node-sass": "^3.13.1",
    "nuka-carousel": "^2.0.4",
    "null-loader": "^0.1.1",
