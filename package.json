    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0"
  },
  "dependencies": {
    "@draft-js-plugins/editor": "^4.1.3",
    "@draft-js-plugins/mention": "^5.2.1",
    "@types/draft-js": "^0.11.13",
    "@types/react-datepicker": "^4.15.1",
    "@types/react-slick": "^0.23.10",
    "cropperjs": "^1.5.13",
    "diff-match-patch": "^1.0.5",
    "draft-js": "^0.11.7",
    "draft-js-export-html": "^1.4.1",
    "draft-js-import-html": "^1.4.1",
    "react-datepicker": "^4.15.0",
    "react-popper": "^2.3.0",
    "react-select": "^5.7.3",
    "react-slick": "^0.29.0",
    "react-toastify": "^9.1.3",
    "slick-carousel": "^1.8.1"
  },
  "peerDependencies": {
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "engines": {
    "node": "^18"
  },
  "overrides": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "browserslist": [
    "last 2 versions",
    "> 5%",
    "not dead",
    "not kaios > 0",
    "not and_chr > 0",
    "not and_ff > 0",
    "not and_qq > 0",
    "not and_uc > 0",
    "not op_mob > 0",
    "not android > 0",
    "not ie_mob > 0",
    "not ios_saf > 0",
    "not op_mini > 0"
  ]
}
