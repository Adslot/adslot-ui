    "react-hot-loader": "^1.3.1",
    "react-icheck": "^0.3.8",
    "react-redux": "^4.4.8",
    "react-select": "^1.0.0-rc.5",
    "react-test-renderer": "^15.6.1",
    "redux": "^3.7.2",
    "rimraf": "^2.6.1",
    "sass-lint": "^1.10.2",
    "sass-loader": "^4.1.1",
    "seamless-immutable": "^6.3.0",
    "semver": "^5.4.1",
    "shelljs": "^0.7.8",
    "sinon": "^2.4.1",
