{
  "name": "adslot-ui",
  "version": "11.6.0",
  "description": "Core component library. By Adslot",
  "main": "dist/adslot-ui-main.js",
  "license": "MIT",
  "scripts": {
    "check-commit": "node scripts/commit-msg",
    "codecov": "cat coverage/coverage-final.json | codecov",
    "dist": "webpack --bail --env=dist",
    "jscs-fix": "jscs ./src ./test --fix",
    "jscs-lint": "jscs ./src ./test",
    "lint": "eslint ./src --rulesdir ./src && eslint ./test --rulesdir ./test",
    "posttest": "npm run lint && npm run sass-lint && npm run jscs-lint && npm run check-commit",
    "profile": "webpack --env=dist --profile --json > stats.json",
    "release:major": "npm version major && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:patch": "npm version patch && npm publish && git push --follow-tags",
    "sass-lint": "sass-lint './src/**/*.scss' -v",
    "start": "node server.js --env=dev",
    "start:cold": "node server.js --env=dev-cold",
    "test": "karma start",
    "test:watch": "karma start --autoWatch=true --singleRun=false"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Adslot/adslot-ui.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "UI Library",
    "components"
  ],
  "author": "Adslot",
  "devDependencies": {
    "alexandria-adslot": "^3.3.2",
    "autoprefixer": "^6.3.4",
    "babel-core": "^6.7.2",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-strict-mode": "^6.6.5",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "bootstrap-sass": "^3.3.6",
    "chai": "^3.5.0",
    "classnames": "^2.2.3",
    "codecov.io": "^0.1.6",
    "css-loader": "^0.23.1",
    "enzyme": "^2.2.0",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-import-resolver-webpack": "^0.3.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.6.1",
    "eslint-plugin-jsx-a11y": "^1.0.4",
    "eslint-plugin-lodash": "^1.8.3",
