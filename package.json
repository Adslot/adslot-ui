{
  "name": "adslot-ui",
  "version": "34.0.5",
  "description": "Core component library. By Adslot",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "es/index.d.ts",
  "files": [
    "dist",
    "src/styles",
    "lib",
    "es",
    "system"
  ],
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist/* docs/* lib/* es/*",
    "generate-types": "node ./scripts/generate-types/generateTypes.js",
    "copy-types": "npm run generate-types -- -c",
    "type-check": "tsc --allowSyntheticDefaultImports --noemit",
    "deps": "ADBLOCK=true DISABLE_OPENCOLLECTIVE=true npm ci --update-notifier=false",
    "dist:prod": "cross-env NODE_ENV=dist node ./scripts/build.js",
    "dist:lib": "cross-env NODE_ENV=cjs node ./scripts/babel-build.js",
    "dist:es": "cross-env NODE_ENV=esm node ./scripts/babel-build.js",
    "dist:docs": "storybook build -o docs",
    "dist": "npm run clean && npm run copy-types && npm run dist:lib && npm run dist:es && npm run dist:prod && npm run dist:docs",
    "dist:system": "cd ./system && npm run system:dist",
    "lint:eslint": "eslint --ext .jsx,.js,.ts ./src && eslint --ext .jsx,.js ./www",
    "lint:stylelint": "stylelint '{src,www}/**/*.css'",
    "lint:prettier": "prettier '{src,www,config}/**/*.{js,json,jsx,md,mdx,css}' --check",
    "format": "prettier '{src,www,config}/**/*.{js,json,jsx,md,mdx,css}' --write",
    "lint": "npm run lint:eslint && npm run lint:prettier && npm run lint:stylelint",
    "prettier": "prettier \"{src,www,config}/**/*.{js,json,jsx,md,mdx,css}\" --write",
    "svgo": "svgo --recursive --multipass --folder=src --config=svgo-config.js",
    "postversion": "git push -u origin $(git rev-parse --abbrev-ref HEAD) --follow-tags && npm publish && echo '…released.'",
    "preversion": "echo 'Releasing…' && ADBLOCK=true DISABLE_OPENCOLLECTIVE=true npm ci --update-notifier=false",
    "release:major": "npm version major -m 'build: release major version %s'",
    "release:minor": "npm version minor -m 'build: release minor version %s'",
    "release:patch": "npm version patch -m 'build: release patch version %s'",
    "start": "storybook dev -p 6006",
    "test": "jest --watch",
    "coverage": "jest --colors --coverage",
    "version": "echo '…generating dist…' && npm run dist",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Adslot/adslot-ui.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 120,
    "arrowParens": "always"
  },
  "lint-staged": {
    "{src,www}/**/*.css": [
      "stylelint --fix"
    ],
    "{src,www,config}/**/*.{js,json,jsx,md,mdx,css}": [
      "prettier --write"
    ],
    "**/*.ts": [
      "npm run type-check"
    ]
  },
  "keywords": [
    "UI Library",
    "components"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "author": "Adslot",
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.0",
    "@babel/generator": "^7.23.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.22.15",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-react": "^7.22.15",
    "@commitlint/cli": "^17.7.2",
    "@commitlint/config-conventional": "^17.7.0",
    "@storybook/addon-a11y": "^7.4.5",
    "@storybook/addon-essentials": "^7.4.5",
    "@storybook/addon-interactions": "^7.4.5",
    "@storybook/addon-links": "^7.4.5",
    "@storybook/blocks": "^7.4.5",
    "@storybook/react": "^7.4.5",
    "@storybook/react-vite": "^7.4.5",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "axios": "^1.5.1",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-inline-react-svg": "^2.0.2",
    "babel-plugin-istanbul": "^6.1.1",
    "babel-plugin-jsx-remove-data-test-id": "^3.0.0",
    "babel-plugin-transform-remove-imports": "^1.7.0",
    "buffer": "^6.0.3",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "classnames": "^2.3.2",
    "commander": "^11.0.0",
    "core-js": "^3.33.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "date-fns": "^2.30.0",
    "eslint": "^8.44.0",
    "eslint-config-adslot": "^1.6.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-jest": "^27.4.2",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-storybook": "^0.6.12",
    "eslint-plugin-testing-library": "^5.11.0",
    "glob": "^8.1.0",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fail-on-console": "^3.1.1",
    "lint-staged": "^14.0.1",
    "mini-css-extract-plugin": "^2.7.6",
    "null-loader": "^4.0.1",
    "postcss": "^8.4.31",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-import": "^15.1.0",
    "postcss-loader": "^7.3.3",
    "postcss-preset-env": "^9.1.4",
    "postcss-safe-parser": "^6.0.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.0.3",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dev-utils": "^12.0.1",
    "react-dom": "^18.2.0",
    "react-html-parser": "^2.0.2",
    "react-router-dom": "^6.10.0",
    "react-to-typescript-definitions": "^3.1.1",
    "rimraf": "^5.0.5",
    "seamless-immutable": "^7.1.4",
    "storybook": "^7.4.5",
    "style-dictionary": "^3.8.0",
    "style-loader": "^3.3.3",
    "stylelint": "^15.10.3",
    "stylelint-config-standard": "^34.0.0",
    "svg4everybody": "^2.1.9",
    "svgo": "^3.0.2",
    "terser-webpack-plugin": "^5.3.9",
    "typescript": "^5.2.2",
    "url-search-params-polyfill": "^8.2.4",
