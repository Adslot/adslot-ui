{
  "name": "adslot-ui",
  "version": "25.0.2",
  "description": "Core component library. By Adslot",
  "main": "dist/adslot-ui-main.js",
  "license": "MIT",
  "scripts": {
    "protect-master": "./scripts/protect-master",
    "check-commit": "node scripts/commit-msg",
    "clean": "rimraf dist/*",
    "codecov": "cat coverage/coverage-final.json | codecov",
    "dist": "./scripts/dist",
    "lint:sass": "sass-lint -v",
    "lint:eslint": "eslint --ext .jsx,.js ./src && eslint --ext .jsx,.js ./docs",
    "lint:prettier": "prettier-check '{src,docs,config}/**/*.{js,json,jsx}'",
    "lint": "npm run lint:eslint && npm run lint:sass && npm run lint:prettier",
    "precommit": "npm run protect-master && lint-staged && npm run check-commit",
    "prettier": "prettier \"{src,docs,config}/**/*.{js,json,jsx}\" --write",
    "posttest": "npm run lint",
    "postversion": "npm publish && git push --follow-tags && echo '…released.'",
    "preversion": "echo 'Releasing…' && git checkout master && git pull && npm i",
    "profile": "webpack --env=dist --profile --json > stats.json",
    "release:major": "npm version major -m 'Release: major version %s.'",
    "release:minor": "npm version minor -m 'Release: minor version %s.'",
    "release:patch": "npm version patch -m 'Release: patch version %s.'",
    "start": "npm run start:dev",
    "start:dev": "webpack-dev-server --config=./config/dev.js",
    "start:cold": "node server.js --env=dev-cold",
    "test": "scripts/test --coverage",
    "test:watch": "scripts/test --watch",
    "version": "echo '…generating dist…' && npm run dist && git add dist/*",
    "scaffold": "./scripts/scaffold"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Adslot/adslot-ui.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 120
  },
  "lint-staged": {
    "{src,docs,config}/**/*.{js,json,jsx}": [
      "prettier --write",
      "git update-index --again"
    ]
  },
  "keywords": [
    "UI Library",
    "components"
  ],
  "author": "Adslot",
  "devDependencies": {
    "autoprefixer": "^7.2.5",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-strict-mode": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "bootstrap-sass": "^3.3.7",
    "chai": "^4.1.2",
    "classnames": "^2.2.5",
    "codecov.io": "^0.1.6",
    "css-loader": "^0.28.9",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-lodash": "^2.6.1",
    "eslint-plugin-react": "^7.6.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "gonzales-pe": "^4.2.3",
    "husky": "^0.14.3",
    "istanbul-instrumenter-loader": "^3.0.0",
    "karma": "^2.0.2",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-webpack": "^2.0.9",
    "lint-staged": "^6.1.0",
    "minimist": "^1.2.0",
    "mocha": "^5.1.1",
    "moment": "^2.20.1",
    "node-sass": "^4.7.2",
    "nuka-carousel": "https://github.com/ChaoLiangSuper/nuka-carousel.git#fix-wrap-around",
    "null-loader": "^0.1.1",
    "object-assign": "^4.1.1",
    "postcss-loader": "^2.1.0",
    "prettier": "^1.10.2",
    "prettier-check": "^2.0.0",
    "prop-types": "^15.6.1",
    "react-bootstrap": "^0.31.5",
    "react-datepicker": "^1.1.0",
    "react-hot-loader": "^3.1.3",
    "react-icheck": "git://github.com/omgaz/react-icheck.git#issue-45-patch-for-react-16-release",
    "react-redux": "^5.0.6",
    "react-select": "^1.2.1",
    "react-syntax-highlighter": "^7.0.0",
    "react-test-renderer": "^16.3.2",
    "redux": "^3.7.2",
    "rimraf": "^2.6.2",
    "sass-lint": "^1.12.1",
    "sass-loader": "^6.0.6",
    "seamless-immutable": "^7.1.3",
    "semver": "^5.5.0",
    "shelljs": "^0.8.1",
    "sinon": "^4.2.2",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "diff-match-patch": "^1.0.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.5",
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  },
  "engines": {
    "node": ">=6 <9"
  },
  "browserslist": [
    "last 2 versions",
    "> 5%",
    "ie >= 11",
    "not and_chr > 0",
    "not and_uc > 0",
    "not android > 0",
    "not ie_mob > 0",
    "not ios_saf > 0",
    "not op_mini > 0"
  ]
}
