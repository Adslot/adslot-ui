{
  "name": "adslot-ui",
  "version": "5.0.1",
  "description": "Core component library. By Adslot",
  "main": "dist/adslot-ui-main.js",
  "license": "MIT",
  "scripts": {
    "start": "node server.js --env=dev",
    "start:cold": "node server.js --env=dev-cold",
    "test": "karma start",
    "test:watch": "karma start --autoWatch=true --singleRun=false",
    "codecov": "cat coverage/coverage-final.json | codecov",
    "posttest": "npm run lint && npm run sass-lint && npm run jscs-lint && npm run check-commit",
    "dist": "webpack --bail --env=dist",
    "profile": "webpack --env=dist --profile --json > stats.json",
    "lint": "eslint ./src ./test",
    "sass-lint": "sass-lint './src/**/*.scss' -v",
    "jscs-lint": "jscs ./src ./test",
    "jscs-fix": "jscs ./src ./test --fix",
    "clean": "rimraf dist/*",
    "check-commit": "./scripts/commit-msg",
    "release:major": "npm version major && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:patch": "npm version patch && npm publish && git push --follow-tags"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Adslot/adslot-ui.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "UI Library",
    "components"
  ],
  "author": "Adslot",
  "devDependencies": {
    "alexandria-adslot": "^1.2.0",
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^6.4.5",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.3",
    "babel-plugin-transform-strict-mode": "^6.5.2",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "bootstrap-sass": "^3.3.6",
    "chai": "^3.5.0",
    "classnames": "^2.2.3",
    "codecov.io": "^0.1.6",
    "css-loader": "^0.23.1",
    "eslint": "^2.2.0",
    "eslint-config-airbnb": "^6.0.1",
    "eslint-loader": "^1.2.1",
    "eslint-plugin-lodash": "^1.1.0",
    "eslint-plugin-react": "^4.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "generator-react-webpack": "^3.2.2",
    "isparta-loader": "^2.0.0",
    "jscs": "^2.10.1",
    "karma": "^0.13.19",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^0.5.3",
    "karma-mocha": "^0.2.1",
    "karma-mocha-reporter": "^1.1.5",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-phantomjs-shim": "^1.2.0",
    "karma-webpack": "^1.7.0",
    "minimist": "^1.2.0",
    "mocha": "^2.4.5",
    "node-sass": "^3.4.2",
    "null-loader": "^0.1.1",
    "object-assign": "^4.0.1",
    "open": "^0.0.5",
    "phantomjs-prebuilt": "^2.1.3",
    "react-addons-test-utils": "^0.14.7",
    "react-bootstrap": "^0.28.2",
    "react-hot-loader": "^1.3.0",
    "react-icheck": "^0.2.1",
    "react-select": "^1.0.0-beta8",
    "react-toggle": "^2.0.1",
    "rimraf": "^2.5.1",
    "sass-lint": "^1.5.0",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.12",
    "webpack-dev-server": "^1.14.1",
    "webpack-sources": "^0.1.1"
  },
  "dependencies": {
    "lodash": "^3.10.1",
    "react": "^0.14.7",
    "react-dom": "^0.14.7"
  }
}
